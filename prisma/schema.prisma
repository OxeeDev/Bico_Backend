generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./banco.db"
}

model User {
  id            Int      @id @default(autoincrement())
  email         String   @unique
  password      String
  image_url     String?
  name          String?
  cpf_or_cnpj   String?  @unique
  age           Int?
  temp_password String?
  number        String?
  sex           String?
  create_at     DateTime @default(now())
}

model User_Worker {
  id            Int      @id @default(autoincrement())
  email         String   @unique
  password      String
  job           String
  image_url     String?
  rating        Float?
  name          String?
  cpf_or_cnpj   String?  @unique
  age           Int?
  temp_password String?
  number        String?
  sex           String?
  create_at     DateTime @default(now())
}
